    public static int ComputeChecksum(byte[] bytes)
    {
        int num = 0;
        for (int index = 0; index < bytes.length; ++index)
            num = (int) ((int) num << 8 ^ (int) Crc16Table[(int) num >> 8 ^ (int) bytes[index]]);
        return num;
    }
}
